pipeline {
    agent any

    stages {
        stage('Hello') 
        {
            steps {
                echo 'Hello World'
            }
        }
        stage('mokthika') 
            {
            steps {
                echo 'this is mokitha file'
            }
        }
        stage('push') 
            {
            steps {
               echo 'this is pushpa file'
            }
        }
    }
}

